    {
		foreground = 216, 101, 84;
	}

	character
    {
		foreground = 216, 101, 84;
	}

	keyword
    {
		foreground = 94, 142, 232;
	}

	constant
    {
		foreground = 188, 117, 198;
	}

	type
    {
		foreground = 125, 105, 220;
