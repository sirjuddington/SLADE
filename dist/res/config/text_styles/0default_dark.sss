
styleset
{
	name = "SLADE (Dark)";

	default
    {
		size = 10;
		foreground = 215, 215, 215;
		background = 37, 37, 37;
		bold = 0;
		italic = 0;
		underlined = 0;
	}

	selection
    {
		background = 72, 72, 72;
	}

	preprocessor
    {
		foreground = 85, 113, 128;
	}

