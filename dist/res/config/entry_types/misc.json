// Misc/General entry types
{
  // Demo lumps
  "demo": {
    "name": "Demo",
    "match_name": [ "demo?", "demo?lmp", "demo_???" ],
    "reliability": 60,
    "export_ext": "lmp",
    "colour": "rgb(255,180,0)"
  },
  "texturex": {
    "name": "TEXTUREx",
    "match_name": [ "TEXTURE1", "TEXTURE2", "TEXTURE3", "TEXTURES" ],
    "format": "texturex",
    "icon": "texturex",
    "extra": [ "textures" ],
    "export_ext": "lmp",
    "min_size": 4,
    "colour": "rgb(255,180,0)"
  },
  "pnames": {
    "name": "PNames",
    "match_name": [ "PNAMES" ],
    "format": "pnames",
    "export_ext": "lmp",
    "min_size": 4,
    "icon": "pnames",
    "colour": "rgb(255,180,0)",
    "editor": "data"
  },
  // Use the same format as PNames, but can't use the texture editor
  "notpnames": {
    "name": "Not PNames",
    "match_name": [ "M_FNAMES", "M_PNAMES", "FLATNAME" ],
    "icon": "pnames",
    "export_ext": "lmp",
    "editor": "data"
  },
  // Miscellaneous binary lumps
  "genmidi": {
    "name": "General MIDI set",
    "match_name": [ "genmidi" ],
    "export_ext": "lmp",
    "icon": "data",
    "size": [ 11908 ],
    "editor": "data"
  },
  "sndcurve": {
    "name": "Sound curve",
    "match_name": [ "sndcurve" ],
    "export_ext": "lmp",
    "icon": "data"
  },
  "rominfo": {
    "name": "ROM Information",
    "match_name": [ "rominfo" ],
    "icon": "data"
  },
  "palette": {
    "name": "Palette",
    "match_extorname": true,
    "match_ext": [ "pal", "act" ],
    "size_multiple": [ 768, 10824 ],
    "editor": "palette",
    "match_name": [ "*pal*", "shadowpage+1" ],
    "export_ext": "pal",
    "icon": "palette",
    "colour": "rgb(255,180,0)"
  },
  "colormap": {
    "inherits": "gfx_raw",
    "name": "Colormap",
    "match_name": [ "colormap", "menucmap", "*map", "tinttab", "xlatab", "lights", "*lite?", "*lite" ],
    "match_ext": [ "cmp" ],
    "match_extorname": true,
    "size": [ 256, 4096, 8192, 8320, 8704, 8448, 8776, 15104, 16384, 16896, 65536 ],
    "reliability": 120,
    "extra": [ "image" ],
    "export_ext": "cmp",
    "category": "Data",
    "icon": "colormap",
    "colour": "rgb(255,180,0)"
  },
  "transtable": {
    "inherits": "colormap",
    "name": "Translation Table",
    "match_name": [ "trantbl*", "tranmap", "cr???*", "*mask", "colort??", "tr_*", "TRANS?0" ],
    "match_extorname": true,
    "extra": [ "image" ],
    "export_ext": "lmp",
    "category": "Data",
    "icon": "palette"
  },
  "ansi": {
    "name": "ANSI Text",
    "size": [ 4000 ],
    "match_name": [ "endoom", "endtext", "endstrf", "loading", "endboom", "doomerr", "duke3d" ],
    "export_ext": "bin",
    "icon": "gfx",
    "editor": "ansi",
    "colour": "rgb(255,180,0)"
  },
  "animated": {
    "name": "Boom Animated",
    "format": "animated",
    "match_name": [ "animated" ],
    "editor": "data",
    "export_ext": "lmp",
    "icon": "animated"
  },
  "switches": {
    "name": "Boom Switches",
    "format": "switches",
    "match_name": [ "switches" ],
    "editor": "data",
    "export_ext": "lmp",
    "icon": "switches"
  },
  "misc_rle0": {
    "name": "RLE0-encoded entry",
    "format": "misc_rle0",
    "icon": "data"
  },
  // Compiled ACS that's not BEHAVIOR
  "acs0": {
    "name": "Compiled ACS (Hexen)",
    "format": "acs0",
    "icon": "map_data",
    "export_ext": "lmp",
    "category": "Maps"
  },
  "acsl": {
    "name": "Compiled ACS (ZDoom)",
    "format": "acsl",
    "icon": "map_data",
    "export_ext": "lmp",
    "category": "Maps"
  },
  "acse": {
    "name": "Compiled ACS (Enhanced)",
    "format": "acse",
    "icon": "map_data",
    "export_ext": "lmp",
    "category": "Maps"
  },
  // This one could be anything
  "rawdata": {
    "name": "Raw Data",
    "match_ext": [ "raw" ],
    "export_ext": "raw",
    "icon": "data",
    // Detect anything else over this
    "reliability": 1
  },
  "d3d_tables_dat": {
    "name": "DN3D Lookup Tables",
    "match_name": [ "tables" ],
    "size": [ 8448 ],
    "icon": "data"
  },
  // misc SRB2
  "sonic_fade": {
    "name": "Fading Mask",
    "icon": "gfx",
    "section": [ "fa", "fades" ],
    "format": "img_raw",
    "image_format": "raw_flat",
    "export_ext": "raw",
    "editor": "gfx",
    "size": [ 4000, 16000, 64000, 256000 ],
    "extra": [ "image" ]
  }
}
